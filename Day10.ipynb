    "def format_name(f_name, l_name):\n",
    "    formated_f_name = f_name.title()\n",
    "    formated_l_name = l_name.title()\n",
    "    \n",
    "    return f\"{formated_f_name} {formated_l_name}\"\n",
    "print(format_name(\"CRISCIANO\", \"RONALDO\"))\n",
    "print(format_name(\"LEONA\",\"MESSI\"))"
   ]
  }
